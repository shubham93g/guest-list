# Google Sheets
GOOGLE_SHEET_ID=
GOOGLE_SERVICE_ACCOUNT_EMAIL=
# Paste raw multi-line PEM string in Vercel dashboard.
# In .env.local, use \n-escaped single line: "-----BEGIN RSA PRIVATE KEY-----\nMIIE...\n-----END RSA PRIVATE KEY-----\n"
GOOGLE_PRIVATE_KEY=

# Twilio Verify (WhatsApp OTP)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_VERIFY_SERVICE_SID=

# JWT session signing (generate with: openssl rand -hex 32)
JWT_SECRET=

# Public env vars (available in browser — shown on the landing page)
NEXT_PUBLIC_COUPLE_NAMES=
NEXT_PUBLIC_WEDDING_DATE=

# Mock mode — bypasses Google Sheets and Twilio (set both to true together)
MOCK_MODE=
NEXT_PUBLIC_MOCK_MODE=
NEXT_PUBLIC_MOCK_OTP=000000

# Mock data — only used when MOCK_MODE=true (keep personal details out of code)
MOCK_GUEST_NAME=
MOCK_EVENT_COUPLE_NAMES=
MOCK_EVENT_DATE=
MOCK_EVENT_DAY=
MOCK_EVENT_VENUE_NAME=
MOCK_EVENT_VENUE_CITY=
